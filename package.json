        "babel-plugin-module-alias": "^1.6.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "compression": "^1.7.1",
        "concurrently": "^3.5.1",
        "express": "^4.15.4",
        "express-static-gzip": "^0.3.2",
        "lodash": "^4.17.4",
        "nodemon": "^1.14.11",
        "npm-run-all": "^4.1.2",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-helmet": "^5.2.0",
